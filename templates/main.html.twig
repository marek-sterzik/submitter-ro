{% extends "base.html.twig" %}
{% block body %}
    <div  class="telo">
        {% if user is not null %}
        {{ user.prettyPrint()|raw }}
        {% endif %}
        <div class="ptacek-kontejner">
            <object class="ptacek" type="image/svg+xml" data="{{ asset('svg/ptacek.svg') }}"></object>
            <div id="prepinac">
                <a href="#">Myslím, že ten pták je infantilní!</a>
            </div>
        </div>
        <div class="sloupek-kontejner">
            <video class="sloupek"><source src="{{ asset('media/sloupek.mp4') }}" type="video/mp4" /></video>
            <div id="prepinac-zpet">
                <a href="#">Tvl, to raději toho ptáka!</a>
            </div>
        </div>

    </div>
    <audio id="sound"><source src="{{ asset('media/tweet.mp3') }}" type="audio/mpeg"></audio>
{% endblock %}
